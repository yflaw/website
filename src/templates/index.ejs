<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title><%= title.index %></title>
        <% include ./partials/head.ejs %>
    </head>
    <body class="index">
        <div class="wrap">
            <% include ./partials/header.ejs %>
            <div class="body">
                <div class="content">
                    <% for (var i = 0; i < index.length; i += 1) { %>
                        <<%= index[i].tag %>>
                        <%= index[i].content %>
                        </<%= index[i].tag %>>
                    <% } %>
                </div>
                <% include ./partials/sidebar.ejs %>
            </div>
        </div>
        <% include ./partials/footer.ejs %>
    </body>
    <script src="./script.js" inline></script>
</html>
